{
  "name": "psstore",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.4.0",
    "yarn": "^1.15.2"
  },
  "dependencies": {
    "luxon": "^1.17.2",
    "query-string": "^6.8.3",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-visibility-observer": "^1.0.3"
  },
  "devDependencies": {
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "s": "yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --config package.json 'src/**/*.{js,css}' --write",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t registry.gitlab.com/jonikanerva/dok/psstore:latest .",
    "docker:push": "docker push registry.gitlab.com/jonikanerva/dok/psstore:latest",
    "deploy": "run-s build docker:build docker:push"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
