{
  "name": "psstore",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.16.2",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "luxon": "^1.23.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-visibility-observer": "^1.0.4"
  },
  "devDependencies": {
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "s": "yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --config package.json 'src/**/*.{js,css}' --write",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t docker.pkg.github.com/jonikanerva/dok8s/psstore:latest .",
    "docker:push": "docker push docker.pkg.github.com/jonikanerva/dok8s/psstore:latest",
    "deploy": "run-s build docker:build docker:push"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
